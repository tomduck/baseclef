<!DOCTYPE html>
<html$if(lang)$ lang="$lang$"$endif$>


<head>

  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=yes">

  <!--[if lt IE 9]>
    <script src="/javascript/html5shiv.js"></script>
  <![endif]-->

$if(title)$
  <title>$title$</title>
$endif$
$if(description)$
  <meta name="description" content="$description$" />
$else$$if(subtitle)$
  <meta name="description" content="$subtitle$" />
$endif$$endif$

  $schemameta$

  $ogmeta$

  $cardmeta$

  <!-- CSS -->
  <style type="text/css">code{white-space: pre;}</style>
$for(css)$
  <link rel="stylesheet" href="$css$">
$endfor$

</head>


<body>

  <div class="container">


    <!-- Page header  - - - - - - - - - - - - - - - - - - - - - - - - - - - -->

    <div class="row">

      <div class="one columns nocollapse"></div>

      <div class="ten columns centered-text">
        <header>
          <hr class="u-full-width" />
          <h1 id="title"><a href="https://tomduck.github.io/bassclef/">Bassclef CMS</a></h1>
          <hr class="u-full-width" />
        </header>
      </div> <!-- class="ten columns centered-text" -->

      <div class="one columns nocollapse"></div>

    </div> <!-- class="row" -->


    <!-- Page content - - - - - - - - - - - - - - - - - - - - - - - - - - - -->

    <div class="row">

      <div class="two columns nocollapse"></div>

      <div class="eight columns">

$if(title)$
        <h1>$title$</h1>

$endif$
$if(socialwidgets)$
        <div class="social">
          $socialwidgets$
        </div> <!-- class="social" -->

$endif$
        <div class="content-body">

          $body$

        </div> <!-- class="content-body" -->

      </div> <!-- class="eight columns" -->

      <div class="two columns nocollapse"></div>

    </div> <!-- class="row" -->


    <!-- Page footer  - - - - - - - - - - - - - - - - - - - - - - - - - - - -->

    <div class="row">

      <div class="one columns nocollapse"></div>

      <div class="ten columns centered-text">
        <footer>
          <hr />
          <p>
            <a href="https://tomduck.github.io/bassclef/">
              <img src="/images/originals/powered-by-bassclef.png" alt="powered-by-bassclef" id="powered-by" />
            </a>
          </p>
        </footer>
      </div> <!-- class="ten columns centered-text" -->

      <div class="one columns nocollapse"></div>

    </div> <!-- class="row" -->


    <!-- - - - - - - - - - - - - - - - - - -  - - - - - - - - - - - - - - - -->

  </div> <!-- class="container" -->

</body>


</html>
